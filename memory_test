S_0x148e09760 .scope module, "memory_tb" "memory_tb" 2 3;
 .timescale -9 -12;
v0x148e20cb0_0 .var "addr", 7 0;
v0x148e20d60_0 .var "clk", 0 0;
v0x148e20df0_0 .var "din", 31 0;
v0x148e20ec0_0 .net "dout", 31 0, v0x148e20a00_0;  1 drivers
v0x148e20f70_0 .var "we", 0 0;
S_0x148e075c0 .scope module, "uut" "memory" 2 10, 3 1 0, S_0x148e09760;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "we";
    .port_info 2 /INPUT 8 "addr";
    .port_info 3 /INPUT 32 "din";
    .port_info 4 /OUTPUT 32 "dout";
v0x148e077b0_0 .net "addr", 7 0, v0x148e20cb0_0;  1 drivers
v0x148e208c0_0 .net "clk", 0 0, v0x148e20d60_0;  1 drivers
v0x148e20960_0 .net "din", 31 0, v0x148e20df0_0;  1 drivers
v0x148e20a00_0 .var "dout", 31 0;
v0x148e20ab0 .array "men", 0 255, 31 0;
v0x148e20b90_0 .net "we", 0 0, v0x148e20f70_0;  1 drivers
E_0x148e10dd0 .event posedge, v0x148e208c0_0;
    .scope S_0x148e075c0;
T_0 ;
    %wait E_0x148e10dd0;
    %load/vec4 v0x148e20b90_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x148e20960_0;
    %load/vec4 v0x148e077b0_0;
    %pad/u 10;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x148e20ab0, 0, 4;
T_0.0 ;
    %load/vec4 v0x148e077b0_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x148e20ab0, 4;
    %assign/vec4 v0x148e20a00_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x148e09760;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x148e20d60_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x148e20d60_0;
    %inv;
    %store/vec4 v0x148e20d60_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x148e09760;
T_2 ;
    %vpi_call 2 24 "$dumpfile", "memory_test.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x148e09760 {0 0 0};
    %vpi_call 2 27 "$monitor", "time=%0t, we=%b, addr=%h, din=%h, dout=%h", $time, v0x148e20f70_0, v0x148e20cb0_0, v0x148e20df0_0, v0x148e20ec0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x148e20f70_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x148e20cb0_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x148e20df0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x148e20f70_0, 0, 1;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x148e20cb0_0, 0, 8;
    %pushi/vec4 3735928559, 0, 32;
    %store/vec4 v0x148e20df0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x148e20f70_0, 0, 1;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x148e20cb0_0, 0, 8;
    %pushi/vec4 3405691582, 0, 32;
    %store/vec4 v0x148e20df0_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x148e20f70_0, 0, 1;
